#!/usr/bin/env node
'use strict';

// Require pkg
var pkg = require('../package.json');

// Require commander
var program = require('commander');

program
  .version(pkg.version)
  .command('init', 'Init delights directory environment')
  .command('dev', 'Compile source code for develop')
  .command('release', 'Compile source code for release')
  .parse(process.argv);
